<odoo>
  <data>
    <record id="cl_import_stock_picking_form" model="ir.ui.view">
      <field name="name">cl_import_stock.picking.form</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form"/>
      <field name="arch" type="xml">
        <xpath expr="//header" position="inside">
          <button name="%(cl_import.import_file)d" string="Importal excel" type="action" class="oe_highlight" context="{'default_move_lines': move_lines, 'default_product':product_id}"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>